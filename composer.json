{
	"name": "finesse/wired",
	"description": "Light standalone ORM with a query builder and no external configuration",
	"keywords": ["orm", "sql", "query builder", "database", "mysql", "sqlite", "pdo"],
	"type": "library",
	"homepage": "https://github.com/Finesse/Wired",
	"license": "MIT",
	"authors": [
		{
			"name": "Surgie Finesse",
			"email": "finesserus@gmail.com"
		}
	],
	"require": {
		"php": ">=7.0",
		"finesse/mini-db": "^0.7.0"
	},
	"require-dev": {
		"ext-pdo_sqlite": "*",
		"phpunit/phpunit": "^6.4",
		"pagerfanta/pagerfanta": "^1.0 || ^2.0"
	},
	"suggest": {
		"pagerfanta/pagerfanta": "To make pagination"
	},
	"autoload": {
		"psr-4": {
			"Finesse\\Wired\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Finesse\\Wired\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	}
}
